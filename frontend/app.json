{
  "expo": {
    "name": "TapIn",
    "slug": "tap-in",
    "version": "14.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tapin",
      "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.TapIn.myapp",
        "buildNumber": "10",
        "infoPlist": {
          "NSLocationWhenInUseUsageDescription": "Allow Tap In to access your location to find nearby users and chat rooms.",
          "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow Tap In to access your location even when the app is in the background.",
          "NSLocationAlwaysUsageDescription": "Allow Tap In to access your location even when the app is in the background.",
          "ITSAppUsesNonExemptEncryption": false
        }
      },
      "android": {
        "edgeToEdgeEnabled": true,
        "adaptiveIcon": {
          "foregroundImage": "./assets/images/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        },
        "permissions": [
          "ACCESS_COARSE_LOCATION",
          "ACCESS_FINE_LOCATION",
          "FOREGROUND_SERVICE",
          "android.permission.ACCESS_COARSE_LOCATION",
          "android.permission.ACCESS_FINE_LOCATION"
        ],
        "package": "com.TapIn.myapp"
      },
      "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./assets/images/favicon.png"
      },
      "plugins": [
        "expo-router",
        "expo-image-picker",
        [
          "expo-location",
          {
            "locationAlwaysPermission": "Allow Tap In to access your location even when the app is in the background.",
            "locationWhenInUsePermission": "Allow Tap In to access your location to find nearby users and chat rooms.",
            "isIosBackgroundLocationEnabled": false,
            "isAndroidBackgroundLocationEnabled": false
          }
        ]
      ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "09438138-397a-4217-ace8-19c2d7a3b92d"
      }
    },
    "owner": "hodanmo15"
  }
}
