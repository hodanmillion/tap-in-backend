{
  "expo": {
    "name": "TapIn",
    "slug": "tap-in",
    "version": "15.2.0",
    "orientation": "portrait",
    "icon": "./frontend/assets/images/icon.png",
    "scheme": "tapin",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./frontend/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6366f1",
      "dark": {
        "image": "./frontend/assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#1e1b4b"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
      "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.TapIn.myapp",
        "buildNumber": "195",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow TapIn to access your location to find nearby users and chat rooms.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow TapIn to access your location even when the app is in the background.",
        "NSLocationAlwaysUsageDescription": "Allow TapIn to access your location even when the app is in the background.",
        "NSPhotoLibraryUsageDescription": "Allow TapIn to access your photos to update your profile picture.",
        "NSCameraUsageDescription": "Allow TapIn to use your camera to take a profile picture.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "aps-environment": "production"
      },
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./frontend/assets/images/adaptive-icon.png",
        "backgroundColor": "#6366f1"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.RECORD_AUDIO"
      ],
        "package": "com.tapin.myapp",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./frontend/assets/images/favicon.png"
    },
      "plugins": [
        "expo-router",
        "expo-image-picker",
        "expo-updates",
        [
          "expo-location",
          {
            "locationAlwaysPermission": "Allow TapIn to access your location even when the app is in the background.",
            "locationWhenInUsePermission": "Allow TapIn to access your location to find nearby users and chat rooms.",
            "isIosBackgroundLocationEnabled": false,
            "isAndroidBackgroundLocationEnabled": false
          }
        ],
      [
        "expo-notifications",
        {
          "icon": "./frontend/assets/images/icon.png",
          "color": "#6366f1",
          "sounds": [],
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-font",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "09438138-397a-4217-ace8-19c2d7a3b92d"
      }
    },
    "owner": "hodanmo15",
    "updates": {
      "url": "https://u.expo.dev/09438138-397a-4217-ace8-19c2d7a3b92d"
    }
  }
}
